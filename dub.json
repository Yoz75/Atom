	"description": "particles simulation (like atoms lol)",
	"license": "MIT",
	"buildTypes": {
		"debug": {
			"dflags": [
				"-g",
				"--mscrtlib=msvcrt",
				"./source/raylib/include/raylib.c",
				"--vgc"
			]
		},
		"debug-cuda": {
			"dflags": [
				"-g",
				"--mscrtlib=msvcrt",
				"./source/raylib/include/raylib.c"
			]
		},
		"release": {
			"dflags": [
				"-O3",
				"-release",
				"-boundscheck=off",
				"./source/raylib/include/raylib.c"
			]
		},
		"release-avx2": {
			"dflags": [
				"-O3",
				"-release",
				"-boundscheck=off",
				"--mattr=+avx2",
				"./source/raylib/include/raylib.c"
			]
		}
    },
	"libs": ["raylibdll"],
	"targetPath":"./bin",
	"name": "atom"
}